-- ***************  Create table definitions  ******************

-- *** Create table OnyakTechFLSRAttributes ***

if not exists (select * from dbo.sysobjects where id = object_id(N'{databaseOwner}{objectQualifier}OnyakTechFLSRAttributes') and OBJECTPROPERTY(id, N'IsTable') = 1)
	CREATE TABLE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes (
		 [AttributeId] int IDENTITY (1, 1) NOT NULL,
		 [PortalId] int NOT NULL,
		 [ModuleId] int NOT NULL,
		 [Comments] nvarchar(500) NOT NULL,
		 [Name] nvarchar(75) NOT NULL,
		 [ValueType] int NOT NULL,
		 [AttrValues] nvarchar(MAX) NOT NULL,
		 [IsSQLLoaded] bit NOT NULL
	) ON [PRIMARY]
GO

if not exists (select * from dbo.sysobjects where id = object_id(N'PK_{objectQualifier}OnyakTechFLSRAttributes') and OBJECTPROPERTY(id, N'IsPrimaryKey') = 1)
	ALTER TABLE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes ADD CONSTRAINT 
		PK_{objectQualifier}OnyakTechFLSRAttributes PRIMARY KEY CLUSTERED (AttributeId) ON [PRIMARY]
GO

-- *** Create table OnyakTechFLSRCategoryAttributes ***

if not exists (select * from dbo.sysobjects where id = object_id(N'{databaseOwner}{objectQualifier}OnyakTechFLSRCategoryAttributes') and OBJECTPROPERTY(id, N'IsTable') = 1)
	CREATE TABLE {databaseOwner}{objectQualifier}OnyakTechFLSRCategoryAttributes (
		 [CategoryAttributeId] int IDENTITY (1, 1) NOT NULL,
		 [AttributeId] int NOT NULL,
		 [Roles] nvarchar(255) NOT NULL,
		 [Enabled] bit NOT NULL,
		 [ViewOrder] int NOT NULL,
		 [Required] bit NOT NULL,
		 [FileCategoryId] int NOT NULL
	) ON [PRIMARY]
GO

if not exists (select * from dbo.sysobjects where id = object_id(N'PK_{objectQualifier}OnyakTechFLSRCategoryAttributes') and OBJECTPROPERTY(id, N'IsPrimaryKey') = 1)
	ALTER TABLE {databaseOwner}{objectQualifier}OnyakTechFLSRCategoryAttributes ADD CONSTRAINT 
		PK_{objectQualifier}OnyakTechFLSRCategoryAttributes PRIMARY KEY CLUSTERED (CategoryAttributeId) ON [PRIMARY]
GO

-- *** Create table OnyakTechFLSRFileHeaderAttributes ***

if not exists (select * from dbo.sysobjects where id = object_id(N'{databaseOwner}{objectQualifier}OnyakTechFLSRFileHeaderAttributes') and OBJECTPROPERTY(id, N'IsTable') = 1)
	CREATE TABLE {databaseOwner}{objectQualifier}OnyakTechFLSRFileHeaderAttributes (
		 [FileAttributeId] int IDENTITY (1, 1) NOT NULL,
		 [PortalId] int NOT NULL,
		 [ModuleId] int NOT NULL,
		 [FileId] int NOT NULL,
		 [UserId] int NOT NULL,
		 [DateModified] datetime NOT NULL,
		 [AttributeValue] nvarchar(MAX) NOT NULL,
		 [CategoryAttributeId] int NOT NULL,
		 [FileCategoryId] int NOT NULL
	) ON [PRIMARY]
GO

if not exists (select * from dbo.sysobjects where id = object_id(N'PK_{objectQualifier}OnyakTechFLSRFileHeaderAttributes') and OBJECTPROPERTY(id, N'IsPrimaryKey') = 1)
	ALTER TABLE {databaseOwner}{objectQualifier}OnyakTechFLSRFileHeaderAttributes ADD CONSTRAINT 
		PK_{objectQualifier}OnyakTechFLSRFileHeaderAttributes PRIMARY KEY CLUSTERED (FileAttributeId) ON [PRIMARY]
GO

