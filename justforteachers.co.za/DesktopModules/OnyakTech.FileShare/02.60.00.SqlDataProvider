-- ***************  Create stored procedures  ******************

-- *** Create stored procedures for table OnyakTechFLSRAttributes ***

CREATE PROCEDURE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributesAdd
		@PortalId int,
		@ModuleId int,
		@Comments nvarchar(500),
		@Name nvarchar(75),
		@ValueType int,
		@AttrValues nvarchar(MAX),
		@IsSQLLoaded bit
	AS
	SET NOCOUNT ON
	INSERT INTO {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes
		(PortalId,ModuleId,Comments,Name,ValueType,AttrValues,IsSQLLoaded)
		VALUES
		(@PortalId,@ModuleId,@Comments,@Name,@ValueType,@AttrValues,@IsSQLLoaded)
	SELECT SCOPE_IDENTITY()
GO

CREATE PROCEDURE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributesUpdate
		@AttributeId int,
		@PortalId int,
		@ModuleId int,
		@Comments nvarchar(500),
		@Name nvarchar(75),
		@ValueType int,
		@AttrValues nvarchar(MAX),
		@IsSQLLoaded bit
	AS
	SET NOCOUNT ON
	UPDATE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes SET 
		PortalId=@PortalId,
		ModuleId=@ModuleId,
		Comments=@Comments,
		Name=@Name,
		ValueType=@ValueType,
		AttrValues=@AttrValues,
		IsSQLLoaded=@IsSQLLoaded
	WHERE
		AttributeId=@AttributeId
GO

CREATE PROCEDURE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributesDelete
		@AttributeId int
	AS
	SET NOCOUNT ON
	DELETE FROM {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes WHERE
		AttributeId=@AttributeId
GO

CREATE PROCEDURE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributesGet
		@AttributeId int
	AS
	SET NOCOUNT ON
	SELECT * 
	FROM {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes WHERE
		AttributeId=@AttributeId
GO

CREATE PROCEDURE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributesCountAll
	AS
	SET NOCOUNT ON
	SELECT count(*) FROM {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes
GO

CREATE PROCEDURE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributesGetAll
	AS
	SET NOCOUNT ON
	SELECT * 
	FROM {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes 
GO

CREATE PROCEDURE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributesDeleteAll
	AS
	SET NOCOUNT ON
	DELETE FROM {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes
GO

CREATE PROCEDURE {databaseOwner}{objectQualifier}OnyakTechFLSRAttributesGetAttributesByModuleId
		@ModuleId int
	AS
	SET NOCOUNT ON
	SELECT * 
	FROM {databaseOwner}{objectQualifier}OnyakTechFLSRAttributes 
	WHERE ModuleId=@ModuleId
GO

