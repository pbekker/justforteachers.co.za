if '{objectQualifier}' <> ''
BEGIN TRY
	IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}EasyDNNNewsCalendarSettings]') AND type in (N'U'))
	BEGIN
		IF EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[EasyDNNNewsCalendarSettings]') AND type in (N'U'))
		BEGIN
			EXECUTE sp_rename N'{databaseOwner}[EasyDNNNewsCalendarSettings]', N'{objectQualifier}EasyDNNNewsCalendarSettings', 'OBJECT'
			BEGIN TRY
			EXECUTE sp_rename N'PK_EasyDNNNewsCalendarSettings', N'PK_{objectQualifier}EasyDNNNewsCalendarSettings', 'OBJECT'
			EXECUTE sp_rename N'IX_EasyDNNNewsCalendarSettings', N'IX_{objectQualifier}EasyDNNNewsCalendarSettings', 'OBJECT'
			EXECUTE sp_rename N'FK_EasyDNNNewsCalendarSettings_Modules', N'FK_{objectQualifier}EasyDNNNewsCalendarSettings_Modules', 'OBJECT'
			EXECUTE sp_rename N'FK_EasyDNNNewsCalendarSettings_Portals', N'FK_{objectQualifier}EasyDNNNewsCalendarSettings_Portals', 'OBJECT'
			END TRY
			BEGIN CATCH
			END CATCH
		END
	END
END TRY
BEGIN CATCH
END CATCH
GO

