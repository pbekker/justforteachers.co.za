	BEGIN
		IF EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[EasyDNNNewsMenuSettings]') AND type in (N'U'))
		BEGIN
			EXECUTE sp_rename N'{databaseOwner}[EasyDNNNewsMenuSettings]', N'{objectQualifier}EasyDNNNewsMenuSettings', 'OBJECT'
			BEGIN TRY
			EXECUTE sp_rename N'PK_EasyDNNNewsMenuSettings', N'PK_{objectQualifier}EasyDNNNewsMenuSettings', 'OBJECT'
			EXECUTE sp_rename N'IX_EasyDNNNewsMenuSettings', N'IX_{objectQualifier}EasyDNNNewsMenuSettings', 'OBJECT'
			EXECUTE sp_rename N'FK_EasyDNNNewsMenuSettings_Modules', N'FK_{objectQualifier}EasyDNNNewsMenuSettings_Modules', 'OBJECT'
			EXECUTE sp_rename N'FK_EasyDNNNewsMenuSettings_Portals', N'FK_{objectQualifier}EasyDNNNewsMenuSettings_Portals', 'OBJECT'
			END TRY
			BEGIN CATCH
			END CATCH
		END
	END
END TRY
